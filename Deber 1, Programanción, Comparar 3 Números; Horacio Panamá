
#include <stdio.h>
#include <stdlib.h>

/*
 * 
 */
int main() {
    int a, b, c, mayor ,medio ,menor;
    printf("ingrese primer numero:\n");
    scanf("%d", &a);
    printf("ingrese segundo numero:\n");
    scanf("%d", &b);
    printf("ingrese tercer numero:\n");
    scanf("%d", &c);
    if (a>b&&b>c){
        mayor=a;
        medio=b;
        menor=c;
    }
    if (c>b&&b>a){
        mayor=c;
        medio=b;
        menor=a;
    }
    if (b>c&&c>a){
        mayor=b;
        medio=c;
        menor=a;
    }
    if (b>a&&a>c){
        mayor=b;
        medio=a;
        menor=c;
    }
    if (a>c&&c>b){
        mayor=a;
        medio=c;
        menor=b;
    }
    if (c>a&&a>b){
        mayor=c;
        medio=a;
        menor=b;
    }
    printf("mayor:%d\n",mayor);
    printf("medio:%d\n",medio);
    printf("menor:%d\n",menor);
        
    system("PAUSE");
    return 0;
}
